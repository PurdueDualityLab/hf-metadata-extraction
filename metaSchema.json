{
  "pre_processed_metadata":{
    "domain": {
      "type": "array", 
      "items": {"type": "string"}
    },
    "model_task": {
      "type": "array", 
      "items": {"type": "string"}
    },
    "frameworks": {
      "type": "array",
      "items": {"type": "string"}
    },
    "libraries": {
      "type": "array",
      "items": {"type": "string"}
    }
  },
  "extract_metadata": {
    "datasets": {
      "type": "array",
      "items": {
        "type": "object",
        "properties":{
          "dataset_name": {"type":"string"},
          "link": {"type": "string"},
          "confidence_value": {"type": "number"}
        }
      }
    },
    "license": {
      "type":"object",
      "properties":{
        "license_name": {"type":"string"},
        "confidence_value": {"type": "number"}
      }
    },
    "github": {
      "type": "object",
      "properties":{
        "github_url": {"type": "string"},
        "confidence_value": {"type": "number"}
      }
    },
    "paper": {
      "type": "array",
      "items": {
        "type": "object",
        "properties":{
          "paper title": {"type": "string"},
          "paper_url": {"type": "string"},
          "confidence_value": {"type": "number"}
        }
      }
    },
    "base_model": {
      "type": "object",
      "properties":{
        "model_name": {"type": "string"},
        "confidence_value": {"type": "number"}
      }
    },
    "parameter_count": {
      "type": "object",
      "properties":{
        "answer": {
          "type": "string",
          "pattern": "^(|[0-9]+[MBT])$"
        },
        "confidence_value": {"type": "number"}
      }
    },
    "hardware": {
      "type": "object",
      "properties":{
        "hardware_name": {"type": "string"},
        "hours_used": {"type": "string"},
        "cloud_provider": {"type": "string"},
        "compute_region": {"type": "string"},
        "confidence_value": {"type": "number"}
      }
    },
    "carbon_emitted": {
      "type": "string"
    },
    "hyper_parameters": {
      "items":{
        "type": "object",
        "properties": {
          "hyper_parameter": {"type":"string"},
          "value": {"type":"string"},
          "confidence_value": {"type": "number"}
        },
        "required":["hyper_parameter", "value", "confidence_value"]
      }
    },
    "evaluation": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "test/metric": {"type": "string"},
              "dataset": {"type": "string"},
              "result": {"type": "string"},
              "confidence_value": {"type": "number"}
            },
            "required": ["test", "result"]
          }
        },
        {
          "type": "object",
          "properties":{
            "link": {"type": "string"},
            "confidence_value": {"type": "number"}
          }
        }
      ] 
    },
    "limitation_and_bias": {"type":"string"},
    "demo": {"type":"string"},
    "grant": {"type":"string"},
    "input_format": {"type":"string"},
    "output_format": {"type":"string"}
  }
}