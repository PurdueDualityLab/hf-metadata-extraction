{
  "pre_processed_metadata":{
    "domain": {
      "type": "array", 
      "items": {"type": "string"}
    },
    "model_task": {
      "type": "array", 
      "items": {"type": "string"}
    },
    "frameworks": {
      "type": "array",
      "items": {"type": "string"}
    },
    "libraries": {
      "type": "array",
      "items": {"type": "string"}
    }
  },
  "simple_metadata": {
    "datasets": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "license": {"type": "string"},
    "github": {
      "type": "string", 
      "format": "uri"
    },
    "paper": {
      "type": "string", 
      "format": "uri"
    },
    "upstream_model": {
      "type": "string"
    },
    "parameter_count": {
      "type": "string",
      "pattern": "^(|[0-9]+[MBT])$"
    },
    "hardware": {"type":"string"}
  },
  "complex_metadata":{
    "hyper_parameters": {
      "type": "object",
      "properties": {
        "epochs": {"type":"string"},
        "batch_size": {"type":"string"},
        "learning_rate": {"type":"string"},
        "optimizer": {"type": "string"}
      },
      "additionalProperties": true
    },
    "evaluation": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "test": {"type": "string"},
          "dataset": {"type": "string"},
          "result":{"type": "string"}
        },
      "required": ["test", "results"]
      }
    },
    "limitation_and_bias": {"type":"string"},
    "demo": {"type":"string"},
    "input_format": {"type":"string"},
    "output_format": {"type":"string"}
  }
}